#define GPIO_ACTIVE_HIGH 0
#define GPIO_ACTIVE_LOW 1

/dts-v1/;
#include "zynq-7000.dtsi"

/ {
	model = "JSKJ ZYNQ-CCU-7Z030 board";
	compatible = "xlnx,zynq-zc706", "xlnx,zynq-7000";

	aliases {
		ethernet0 = &gem0;
		serial0 = &uart0;
		spi0 = &qspi;
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x40000000>;
	};

	chosen {
		bootargs = "";
		stdout-path = "serial0:115200n8";
	};

	usb_phy0: phy0@e0002000 {
		compatible = "ulpi-phy";
		#phy-cells = <0>;
		reg = <0xe0002000 0x1000>;
		view-port = <0x0170>;
		drv-vbus;
	};

	led {
		compatible="mlk,led";
		status="okay";
		default-state="on";

		led-gpio = <&gpio0 7 GPIO_ACTIVE_HIGH>;
	};


};

&clkc {
	ps-clk-frequency = <33333333>;
};

&gem0 {
	status = "okay";
	phy-mode = "rgmii-id";
	phy-handle = <&ethernet_phy>;
	local-mac-address = [00 0a 35 00 00 00];
	ethernet_phy: ethernet-phy@1 {
		reg = <1>;
		device_type = "ethernet-phy";
	};
};


&qspi {
	u-boot,dm-pre-reloc;
	status = "okay";
	is-dual = <0>;
	num-cs = <1>;
	flash@0 {
		compatible = "mt25ql256a";
		reg = <0x0>;
		spi-tx-bus-width = <4>;
		spi-rx-bus-width = <4>;
		spi-max-frequency = <125000000>;
		#address-cells = <1>;
		#size-cells = <1>;
		partition@0 {
			label = "qspi-fsbl-uboot";
			reg = <0x0 0x200000>;
		};
		partition@200000 {
			label = "qspi-linux";
			reg = <0x200000 0x620000>;/* 0x420000 */
		};
		partition@620000 {
			label = "qspi-device-tree";
			reg = <0x820000 0x20000>;/* 0x620000 */
		};
		/*
		partition@640000 {
			label = "qspi-rootfs";
			reg = <0x640000 0x400000>;
		};
		*/
	};
};

&uart0 {
        cts-override ;
        device_type = "serial";
        port-number = <0>;
        status = "okay";
        u-boot,dm-pre-reloc;
};

&uart1 {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&usb0 {
 	status = "okay";
	phy_type = "ulpi";
 	dr_mode = "host";
 	usb-phy = <&usb_phy0>;
 };

&watchdog0 {
	reset-on-timeout;
};
