#CONFIG_USB_EHCI_ROOT_HUB_TT=y
# CONFIG_USB_EHCI_TT_NEWSCHED is not set
CONFIG_NOP_USB_XCEIV=y #将 NOP USB 收发器接口支持编译到内核中
